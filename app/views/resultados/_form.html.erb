<%= form_for(@resultado) do |f| %>
  <% if @resultado.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@resultado.errors.count, "error") %> prohibited this resultado from being saved:</h2>

      <ul>
      <% @resultado.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>



  
  <div class="field">
   Circunscripcion<br>
    <%= collection_select @resultado,  :circunscripcion, Circunscripcion.all, :id, :nombre %>
  </div>
	  <div class="field">
    Mesa<br>
    <%= collection_select @resultado,  :mesa, Mesa.all, :nombre, :nombre %>
  </div>
  
  <% Candidato.all.each do |c|%>
  <div class="field">
    <%=c.nombre%><br>
    <%= number_field  c.nombre , nil%>
  </div>
 
  <%end%>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
